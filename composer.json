{
  "name": "sstoye/phpas2",
  "description": "PHPAS2 is a php-based implementation of the EDIINT AS2 standard",
  "version": "1.3.4",
  "authors": [
    {
      "name": "Vladyslav K",
      "role": "Developer",
      "email": "vk.tiamo@gmail.com"
    },
    {
      "name": "Simon Stoye",
      "role": "Developer",
      "email": "sstoye@simonstoye.de"
    }
  ],
  "keywords": [
    "edi",
    "ediint",
    "as2",
    "server"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7",
    "ext-zlib": "*",
    "ext-openssl": "*",
    "psr/log": "^1.0",
    "guzzlehttp/guzzle": "^7.0",
    "phpseclib/phpseclib": "^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "AS2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AS2\\Tests\\": "tests/"
    }
  }
}
